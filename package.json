{
  "name": "argos",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "pretest": "cross-env NODE_ENV=test sequelize db:migrate",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit --maxWorkers=10",
    "posttest": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpileOnly --inspect --ignore node_modules src/server.ts",
    "queue": "ts-node-dev -r tsconfig-paths/register --transpileOnly --inspect=9230 --ignore node_modules src/queue.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "postbuild": "cp -r src/app/views dist/app/",
    "migrations": "sequelize db:migrate",
    "start": "NODE_ENV=production node ./dist/server.js",
    "queue:production": "NODE_ENV=production node ./dist/queue.js",
    "pretest:watch": "cross-env NODE_ENV=test sequelize db:migrate",
    "test:watch": "cross-env NODE_ENV=test jest --watch --detectOpenHandles",
    "posttest:watch": "cross-env NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@sentry/node": "5.15.5",
    "aws-sdk": "^2.673.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bull": "^3.14.0",
    "bull-board": "^0.7.0",
    "connect-datadog": "^0.0.9",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "datadog-winston": "^1.5.0",
    "date-fns": "^2.13.0",
    "dd-trace": "^0.20.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^4.0.4",
    "ioredis": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.6",
    "nodemailer-express-handlebars": "^4.0.0",
    "pg": "^8.1.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.1",
    "socket.io": "^2.3.0",
    "tsconfig-paths": "^3.9.0",
    "winston": "^3.2.1",
    "youch": "^2.0.10",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.12.2",
    "@types/bull-board": "^0.6.0",
    "@types/connect-datadog": "^0.0.4",
    "@types/cors": "^2.8.6",
    "@types/datadog-winston": "^1.0.2",
    "@types/express-handlebars": "^3.1.0",
    "@types/factory-girl": "^5.0.2",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.2",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/multer": "^1.4.3",
    "@types/multer-s3": "^2.7.7",
    "@types/nodemailer": "^6.4.0",
    "@types/sequelize": "^4.28.8",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.9",
    "@types/yup": "^0.28.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.0",
    "eslint-plugin-prettier": "^3.1.3",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "sucrase": "^3.13.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tscpaths": "^0.0.9",
    "typescript": "^3.8.3"
  }
}
