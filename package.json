{
  "name": "argos",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.js",
    "queue": "nodemon src/queue.js",
    "build": "babel src --out-dir dist --copy-files  ",
    "heroku-postbuild": "sucrase ./src -d ./dist --transforms imports && sequelize db:migrate",
    "migrations": "sequelize db:migrate",
    "start": "NODE_ENV=production node ./dist/server.js",
    "queue:production": "NODE_ENV=production node ./dist/queue.js",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit --maxWorkers=10",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",
    "pretest:watch": "NODE_ENV=test sequelize db:migrate",
    "test:watch": "NODE_ENV=test jest --watch --detectOpenHandles",
    "posttest:watch": "NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@sentry/node": "5.15.4",
    "aws-sdk": "^2.654.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bull": "^3.13.0",
    "bull-board": "^0.7.0",
    "connect-datadog": "^0.0.9",
    "cors": "^2.8.5",
    "datadog-winston": "^1.3.1",
    "date-fns": "^2.11.1",
    "dd-trace": "^0.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^4.0.3",
    "ioredis": "^4.16.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.6",
    "nodemailer-express-handlebars": "^4.0.0",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
    "pm2": "^4.2.3",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1",
    "youch": "^2.0.10",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@types/jest": "^25.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^25.2.7",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4",
    "sqlite3": "^4.1.1",
    "sucrase": "^3.13.0",
    "supertest": "^4.0.2"
  }
}
